#include <locale.h>
#include <stdio.h>

void date()
{
	puts("                   _              _   _ ");
	puts("                |  _|   /|  /|   | | |_ ");
	puts("                |   | .  |   | . |_| |_|");
}
void name()
{
	puts("");
	puts("");
	puts("     * * * * * * * * * * * * * * * * * * * * * *");
	puts("     *                                         *");
	puts("     * тема: Разработка консольного приложения *");
	puts("     *                                         *");
	puts("     *        Выполнил Мозгунов А.A.           *");
	puts("     *                                         *");
	puts("     * * * * * * * * * * * * * * * * * * * * * *");
}
void main()
{
	setlocale(LC_ALL, "RUS");
	name();
	date();
	getchar();
}
